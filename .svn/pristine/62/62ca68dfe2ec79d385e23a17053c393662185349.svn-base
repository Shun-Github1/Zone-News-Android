        <attr format="color" name="numColor"/>

        <attr format="string" name="text"/>
        <attr format="string" name="num"/>

        <attr format="dimension" name="numSize"/>
        <attr format="dimension" name="textsSize"/>

        <attr format="dimension" name="labelTopPadding"/>
        <attr format="dimension" name="labelCenterPadding"/>
        <attr format="dimension" name="labelBottomPadding"/>


        <attr name="numStyle">
