    <declare-styleable name="BubbleView">
        <attr format="dimension" name="arrowWidth"/>
        <attr format="dimension" name="angle"/>
        <attr format="dimension" name="arrowHeight"/>
        <attr format="dimension" name="arrowPosition"/>
        <attr format="color" name="bubbleColor"/>
        <attr format="enum" name="arrowLocation">
            <enum name="left" value="0x00"/>
            <enum name="right" value="0x01"/>
            <enum name="top" value="0x02"/>
            <enum name="bottom" value="0x03"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="CircleLayout">
        <attr format="dimension" name="radius"/>
        <attr format="integer" name="offset"/>
    </declare-styleable>
    <declare-styleable name="CircleView">
        <attr format="color" name="circleBack"/>
        <attr format="string" name="circleText"/>
    </declare-styleable>
    <declare-styleable name="CollapsibleTextView">
        <attr format="color" name="suffixColor"/>
        <attr format="integer" name="collapsedLines"/>
        <attr format="string" name="collapsedText"/>
        <attr format="string" name="expandedText"/>
