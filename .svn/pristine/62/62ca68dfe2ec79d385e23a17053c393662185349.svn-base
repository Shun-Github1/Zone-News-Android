    <declare-styleable name="YLCircleImageView">
        <attr format="dimension" name="radiusYL">圆角半径</attr>
        <attr format="dimension" name="topLeftRadiusYL">左上</attr>
        <attr format="dimension" name="topRightRadiusYL">右上</attr>
        <attr format="dimension" name="bottomLeftRadiusYL">左下</attr>
        <attr format="dimension" name="bottomRightRadiusYL">右下</attr>

        <attr format="dimension" name="topLeftRadius_xYL">左上X轴</attr>
        <attr format="dimension" name="topLeftRadius_yYL">左上Y轴</attr>

        <attr format="dimension" name="topRightRadius_xYL">右上X轴</attr>
        <attr format="dimension" name="topRightRadius_yYL">右上Y轴</attr>

        <attr format="dimension" name="bottomLeftRadius_xYL">左下X轴</attr>
        <attr format="dimension" name="bottomLeftRadius_yYL">左下Y轴</attr>

        <attr format="dimension" name="bottomRightRadius_xYL">右下X轴</attr>
        <attr format="dimension" name="bottomRightRadius_yYL">右下Y轴</attr>

        <attr format="dimension" name="borderWidthYL">描边宽度</attr>
        <attr format="color" name="borderColorYL">描边颜色</attr>
        <attr format="dimension" name="borderSpaceYL">描边与图片间距</attr>
        
        <attr format="enum" name="scaleTypeYL">
            <enum name="top" value="0">从图片顶部开始绘制，即肯定会展示图片顶部，默认</enum>
            <enum name="center" value="1">展示图片中心</enum>
            <enum name="bottom" value="2">从图片底部开始绘制，即肯定会展示图片底部</enum>
            <enum name="fitXY" value="3">完全展示，有可能变形</enum>
        </attr>
    </declare-styleable>
