        <attr format="boolean" name="sub_anim"/>
    </declare-styleable>
    <declare-styleable name="ArcImageView">
        <attr format="dimension" name="arcHeight"/>
        <attr format="color" name="bColor"/>
        <attr format="enum" name="mDirection">
            <enum name="out" value="0"/>
            <enum name="inner" value="1"/>
        </attr>
        <attr format="enum" name="mPosition">
            <enum name="left" value="0"/>
            <enum name="top" value="1"/>
            <enum name="right" value="2"/>
            <enum name="bottom" value="3"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ArcProgressBar">
        <attr format="float" name="arc_max_progress"/>
        <attr format="color" name="arc_bg_color"/>
        <attr format="dimension" name="arc_stroke_width"/>
        <attr format="float" name="arc_progress"/>
        <attr format="color" name="arc_progress_color"/>
        <attr format="string" name="arc_first_text"/>
        <attr format="color" name="arc_first_text_color"/>
        <attr format="dimension" name="arc_first_text_size"/>
        <attr format="string" name="arc_second_text"/>
        <attr format="color" name="arc_second_text_color"/>
        <attr format="dimension" name="arc_second_text_size"/>
        <attr format="float" name="arc_angle_size"/>
        <attr format="float" name="arc_start_angle"/>
